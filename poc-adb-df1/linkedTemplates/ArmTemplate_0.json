{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "poc-adb-df1"
		},
		"AzureDataLakeS1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeS1'"
		},
		"LS1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS1'"
		},
		"mdccmysql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'mdccmysql'"
		},
		"AzureBatch1_accessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accessKey' of 'AzureBatch1'"
		},
		"AzureDataLakeS1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://box2pbi.dfs.core.windows.net"
		},
		"AzureBatch1_properties_typeProperties_batchUri": {
			"type": "string",
			"defaultValue": "https://batchpoc1.eastus2.batch.azure.com"
		},
		"AzureBatch1_properties_typeProperties_poolName": {
			"type": "string",
			"defaultValue": "bp100"
		},
		"AzureBatch1_properties_typeProperties_accountName": {
			"type": "string",
			"defaultValue": "batchpoc1"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/AzureDataLakeS1')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('AzureDataLakeS1_properties_typeProperties_url')]",
					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('AzureDataLakeS1_accountKey')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/LS1')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"connectionString": "[parameters('LS1_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/mdccmysql')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureMySql",
				"typeProperties": {
					"connectionString": "[parameters('mdccmysql_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureBatch1')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBatch",
				"typeProperties": {
					"batchUri": "[parameters('AzureBatch1_properties_typeProperties_batchUri')]",
					"poolName": "[parameters('AzureBatch1_properties_typeProperties_poolName')]",
					"accountName": "[parameters('AzureBatch1_properties_typeProperties_accountName')]",
					"linkedServiceName": {
						"referenceName": "LS1",
						"type": "LinkedServiceReference"
					},
					"accessKey": {
						"type": "SecureString",
						"value": "[parameters('AzureBatch1_accessKey')]"
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/LS1')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/DIRSample')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "AzureDataLakeS1",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "Excel",
				"typeProperties": {
					"sheetName": "Sheet1",
					"location": {
						"type": "AzureBlobFSLocation",
						"fileName": "DIR_Sample1.xlsx",
						"folderPath": "MDCC",
						"fileSystem": "supplies"
					},
					"firstRowAsHeader": true
				},
				"schema": [
					{
						"name": "Project Name",
						"type": "String"
					},
					{
						"name": "PO Number",
						"type": "String"
					},
					{
						"name": "Supplier Name",
						"type": "String"
					},
					{
						"name": "Part Num & Rev",
						"type": "String"
					},
					{
						"name": "Part Description",
						"type": "String"
					},
					{
						"name": "Allocation Week",
						"type": "String"
					},
					{
						"name": "Allocation Quantity",
						"type": "String"
					},
					{
						"name": "Shipping Quantity",
						"type": "String"
					},
					{
						"name": "Supplier Location",
						"type": "String"
					},
					{
						"name": "Inspector Name",
						"type": "String"
					},
					{
						"name": "Inspection Date",
						"type": "String"
					},
					{
						"name": "Sample Quantity",
						"type": "String"
					}
				]
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/AzureDataLakeS1')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/mdccmysqltgt')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "mdccmysql",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureMySqlTable",
				"schema": [
					{
						"name": "ProjectName",
						"type": "varchar",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "PONumber",
						"type": "varchar",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "SupplierName",
						"type": "varchar",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "PartNumRev",
						"type": "varchar",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "PartDescription",
						"type": "varchar",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "AllocationWeek",
						"type": "int",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "AllocationQuantity",
						"type": "int",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "ShippingQuantity",
						"type": "int",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "SupplierLocation",
						"type": "varchar",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "InspectorName",
						"type": "varchar",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "InspectionDate",
						"type": "date",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "SampleQuantity",
						"type": "int",
						"precision": 0,
						"scale": 0
					}
				],
				"typeProperties": {
					"tableName": "diminsrep"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/mdccmysql')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/mdccp1')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy MDCC Sample",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "ExcelSource",
								"storeSettings": {
									"type": "AzureBlobFSReadSettings",
									"recursive": true,
									"enablePartitionDiscovery": false
								}
							},
							"sink": {
								"type": "AzureMySqlSink",
								"writeBatchSize": 10000,
								"writeBatchTimeout": "00:00:30"
							},
							"enableStaging": false,
							"enableSkipIncompatibleRow": true,
							"translator": {
								"type": "TabularTranslator",
								"mappings": [
									{
										"source": {
											"name": "Project Name",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "ProjectName",
											"type": "String",
											"physicalType": "varchar"
										}
									},
									{
										"source": {
											"name": "PO Number",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "PONumber",
											"type": "String",
											"physicalType": "varchar"
										}
									},
									{
										"source": {
											"name": "Supplier Name",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "SupplierName",
											"type": "String",
											"physicalType": "varchar"
										}
									},
									{
										"source": {
											"name": "Part Num & Rev",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "PartNumRev",
											"type": "String",
											"physicalType": "varchar"
										}
									},
									{
										"source": {
											"name": "Part Description",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "PartDescription",
											"type": "String",
											"physicalType": "varchar"
										}
									},
									{
										"source": {
											"name": "Allocation Week",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "AllocationWeek",
											"type": "Int32",
											"physicalType": "int"
										}
									},
									{
										"source": {
											"name": "Allocation Quantity",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "AllocationQuantity",
											"type": "Int32",
											"physicalType": "int"
										}
									},
									{
										"source": {
											"name": "Shipping Quantity",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "ShippingQuantity",
											"type": "Int32",
											"physicalType": "int"
										}
									},
									{
										"source": {
											"name": "Supplier Location",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "SupplierLocation",
											"type": "String",
											"physicalType": "varchar"
										}
									},
									{
										"source": {
											"name": "Inspector Name",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "InspectorName",
											"type": "String",
											"physicalType": "varchar"
										}
									},
									{
										"source": {
											"name": "Inspection Date",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "InspectionDate",
											"type": "DateTime",
											"physicalType": "date"
										}
									},
									{
										"source": {
											"name": "Sample Quantity",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "SampleQuantity",
											"type": "Int32",
											"physicalType": "int"
										}
									}
								],
								"typeConversion": true,
								"typeConversionSettings": {
									"allowDataTruncation": true,
									"treatBooleanAsNumber": false
								}
							}
						},
						"inputs": [
							{
								"referenceName": "DIRSample",
								"type": "DatasetReference",
								"parameters": {}
							}
						],
						"outputs": [
							{
								"referenceName": "mdccmysqltgt",
								"type": "DatasetReference",
								"parameters": {}
							}
						]
					}
				],
				"policy": {
					"elapsedTimeMetric": {},
					"cancelAfter": {}
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/DIRSample')]",
				"[concat(variables('factoryId'), '/datasets/mdccmysqltgt')]"
			]
		}
	]
}